{
    "name": "saschati/profit-center-fx-tt-api",
    "description": "Profit Center FX TT API project",
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "doctrine/migrations": "^3.6",
        "symfony/console": "^6.3",
        "psr/container": "2.0.2",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "symfony/error-handler": "^5.2",
        "friendsofphp/php-cs-fixer": "^3.22"
    },
    "autoload": {
        "files": [
            "src/env.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes"
    }
}
